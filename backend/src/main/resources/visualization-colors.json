{
  "theme": "dark",
  "fieldConfig": {
    "defaults": {
      "color": {
        "mode": "palette-classic"
      },
      "custom": {
        "lineWidth": 2,
        "drawStyle": "line",
        "lineInterpolation": "smooth",
        "showPoints": "never",
        "fillOpacity": 35,
        "gradientMode": "opacity"
      }
    },
    "overrides": [
      {
        "matcher": { "id": "byRegexp", "options": ".*error.*" },
        "properties": [
          { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
        ]
      },
      {
        "matcher": { "id": "byRegexp", "options": ".*success.*|.*conversion.*" },
        "properties": [
          { "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }
        ]
      },
      {
        "matcher": { "id": "byRegexp", "options": ".*pending.*|.*waiting.*" },
        "properties": [
          { "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }
        ]
      },
      {
        "matcher": { "id": "byRegexp", "options": ".*revenue.*|.*orders.*|.*aov.*|.*rate.*|.*avg.*" },
        "properties": [
          { "id": "custom.fillOpacity", "value": 45 },
          { "id": "custom.gradientMode", "value": "opacity" }
        ]
      }
    ]
  },
  "palette": [
    "#60A5FA", "#34D399", "#F59E0B", "#A78BFA", "#F43F5E", "#10B981",
    "#06B6D4", "#F97316", "#3B82F6", "#22D3EE", "#84CC16", "#E879F9"
  ]
}
